
public class CalculosMath {
	static double a =	Math.sqrt(103.5);
	public static void main(String[] args) {
		System.out.println(a +"");
		
		double base =5;
		double exponente=5;
		int resultado = (int)Math.pow(base,exponente);
		System.out.println("el resultado es : "+resultado +"");
	}
}
