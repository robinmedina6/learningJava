package clases;

public class Cuadrilatero {
//atributos
	private float lado1;
	private float lado2;

	// Metodos

	// metodo constructor 1 Cuadrilatero
	public Cuadrilatero(float lado1, float lado2) {
		this.lado1 = lado1;
		this.lado2 = lado2;

	}

	// metodo constructor 2 cuadrilatero
	public Cuadrilatero(float lado1) {
		this.lado1= this.lado2= lado1;
	}

}
