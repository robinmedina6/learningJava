package clases;

public class objeto {
	//atributos
	private int posicionx; 
	private int posiciony;
	//metodos
	public void moverDerecha() {
		posicionx++;		
	}
	public void moverIzquierda() {
		posicionx--;
	}
	public void moverArriba() {
		posiciony++;
	}
	public void moverAbajo() {
		posiciony--;
	}
}
